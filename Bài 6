#include <bits/stdc++.h>
using namespace std;

bool tang(string s){
	for(int i=0;i<s.size()-1;i++){
		if(s[i]<s[i+1]) return 0;
	}
	return 1;
}

bool giam(string s){
	for(int i=0;i<s.size()-1;i++){
		if(s[i]>s[i+1]) return 0;
	}
	return 1;
}

int main(){
    int t;
    cin >> t;
    while(t--){
    	string s;
    	cin >> s;
    	if(giam(s) || tang(s)) cout << "Yes" << endl;
    	else cout << "No" << endl;
	}
}
